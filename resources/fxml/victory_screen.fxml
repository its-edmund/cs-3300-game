<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">

    <padding>
        <Insets top="10" bottom="10" left="0" right="0" />
    </padding>

    <top>
          <Text fx:id="titleText" />
    </top>

    <center>
        <HBox alignment="CENTER" spacing="50" fx:id="table">

            <VBox spacing="10"
                  alignment="CENTER"
                  fx:id="playerList"
            />

            <VBox spacing="10"
                  alignment="CENTER"
                  fx:id="resultList"
            />

            <VBox spacing="10"
                  alignment="CENTER"
                  fx:id="awardsList"
            />

        </HBox>

<!--        <TableView  fx:id="table">-->

<!--        </TableView>-->


    </center>

</BorderPane>
